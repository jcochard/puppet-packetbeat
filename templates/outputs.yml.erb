# Begin outputs configuration section
<% scope['packetbeat::outputs_hash'].each_pair do |key, value| -%>
outputs.<%= key %>:
  enabled: true
  <%- unless value.nil? -%>
  <%- value.each_pair do |key, value| -%>
  <%= key %>: <%= value %>
  <%- end -%>
  <%- end -%>
# End outputs configuration section
<%- end -%>
