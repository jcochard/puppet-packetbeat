# Enabled protocols, managed by Puppet
# Changes will be overwritten
<% @protocols_hash.each_pair do |key, value| -%>
packetbeat.protocol.<%= key %>:
  enabled: true
  <%- unless key = 'icmp' -%>
  <%- value.each_pair do |key, value| -%>
  <%= key %>: <%= value %>
  <%- end -%>
  <%- end -%>
<%- end -%>
