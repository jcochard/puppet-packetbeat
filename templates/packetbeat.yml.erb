# Begin packetbeat configuration section
<% scope['packetbeat::config_hash'].each_pair do |key, value| -%>
<%- if value.is_a?(String) -%>
<%= key %>: <%= value %>
<%- else %>
<%= key %>
<%- unless value.nil? -%>
<%- value.each_pair do |key, value| -%>
  <%= key %>: <%= value %>
<%- end -%>
<%- end -%>
<%- end -%>
# End packetbeat configuration section
<%- end %>
